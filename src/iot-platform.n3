	rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
	rdfs:label "Platform" ;
	rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#PlatformSite" .
